{
  "presets": ["es2015", "es2016",
    ["env", {
      "modules": false,
      "targets": {
        "node": "current",
        "uglify": false
			},
			"debug": {
				"sourceMap": "inline",
				"retainLines": true
			}
    }]
	],
  "plugins": [
    "add-module-exports",
    "syntax-dynamic-import"
  ]
}
